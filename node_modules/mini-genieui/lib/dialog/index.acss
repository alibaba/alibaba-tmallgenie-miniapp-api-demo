.g-dialog-show{position:fixed;top:0;left:0;right:0;bottom:0}.g-dialog-hide{display:none}.g-dialog-document,.g-dialog-mask{position:absolute;top:0;left:0;right:0;bottom:0}.g-dialog-mask{background-color:rgba(0,0,0,.75)}.g-dialog-document{display:flex;align-items:center;justify-content:center}.g-dialog-content{height:328px;position:relative;display:flex;flex-direction:column;background-color:#181b23;border-radius:2px;width:580px}.g-dialog-header{font-size:36px;line-height:21px;text-align:center;color:#fff;margin-top:58px;margin-bottom:50px}.g-dialog-footer{display:flex;flex-shrink:0;align-items:center;color:#fff;margin-top:auto;margin-bottom:0;height:88px;position:relative;font-size:18px;background-color:rgba(61,66,77,.6)}.g-dialog-button{flex-grow:1;height:88px;line-height:88px;text-align:center;font-size:28px}.g-dialog-button-left{color:#fff}.g-dialog-button-right{color:#0082ff}.g-dialog-button-left-reverse{color:#d62f2f}.g-dialog-button-right-reverse{color:#fff}.g-dialog-button-divider{width:1px;height:88px;background-color:#222}