.__tag{overflow:hidden;width:92px;height:40px;display:inline-block}.__tag::before{position:absolute;content:'';width:8px;height:25px;background-color:#f1e2c4}.__tag__content{display:flex;justify-content:center;box-sizing:border-box;width:100%;height:100%;border-radius:0 0 8px 8px;transform:rotate(4deg);padding:6px 20px 5px;background-image:linear-gradient(128deg,#f1e2c4 1%,#e0c59d 100%);position:relative;top:-5px;left:1px}.__tag__text{display:inline-block;color:#916433;transform:rotate(-5deg);font-size:26px;font-weight:600}